---
interface Props {
    title: string;
}

const { title } = Astro.props;
---

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>{title}</title>
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
        rel="shortcut icon"
        type="image/x-icon"
        href="https://images.squarespace-cdn.com/content/v1/6226544384704c71b90fb38e/8fa3a4b8-5596-45a1-9e3a-ea10290604c7/favicon.ico?format=100w"
    />
    <link
        href="https://fonts.googleapis.com/css2?family=M+PLUS+1p:wght@800;900&display=swap"
        rel="stylesheet"
    />
    <style is:global>
        @tailwind base;
        @tailwind components;

        @tailwind utilities;

        @page {
            size: auto;
            margin: 0mm;
        }
        .jp-bold {
            font-family: "M PLUS 1p", sans-serif;
            font-weight: 800;
        }

        h4 {
            @apply font-bold;
        }

        h5 {
            @apply italic;
        }

        b {
            font-family: "M PLUS 1p", sans-serif;
            font-weight: 800;
        }
        p {
            margin-bottom: 0.65rem !important;
            margin-top: 0.65rem !important;
        }
        @media print {
            a {
                text-decoration: none !important;
                color: black !important;
            }

            li {
                padding: 0;
            }
        }
    </style>
    <body>
        <div class="prose prose-xl container mx-auto max-w-7xl">
            <h2 class="flex w-full items-baseline justify-between space-x-2">
                <span class="jp-bold">Russians Against War</span>
                <a
                    class="flex flex-col items-center text-blue-500 no-underline print:w-10 print:text-black"
                    href="https://yasoprotivlenie.com"
                    target="_blank"
                >
                    <img
                        class="my-0 w-10 lg:hidden"
                        src="data:image/png;base64,"
                        alt="homepage"
                    />
                    <span class="text-xs underline print:no-underline">HP</span>
                </a>
                <a
                    class="flex flex-col items-center text-blue-500 no-underline print:w-10 print:text-black"
                    href="https://twitter.com/yakinikuprotiv"
                    target="_blank"
                >
                    <img
                        class="my-0 w-10 lg:hidden"
                        src="data:image/png;base64,"
                        alt="twitter"
                    />
                    <span class="text-xs underline print:no-underline"
                        >twitter</span
                    >
                </a>
                <a
                    class="flex flex-col items-center text-blue-500 no-underline print:h-10 print:w-10 print:text-black"
                    href="https://www.instagram.com/ya.soprotivlenie/"
                    target="_blank"
                >
                    <img
                        class="my-0 w-10 lg:hidden"
                        src="data:image/png;base64,"
                        alt="instagram"
                    />
                    <span class="text-xs underline print:no-underline"
                        >insta</span
                    >
                </a>
                <a
                    class="flex flex-col items-center text-blue-500 no-underline print:w-10 print:text-black"
                    href="https://t.me/yasoprotivlenie"
                    target="_blank"
                >
                    <img
                        class="my-0 w-10 lg:hidden"
                        src="data:image/png;base64,"
                        alt="telegram"
                    />
                    <span class="text-xs underline print:no-underline">tg</span>
                </a>
            </h2>
        </div>
        <article
            class="lg:prose-xl prose-xl container mx-auto mt-6 max-w-7xl text-sm"
        >
            <div class="flex flex-row space-x-2">
                <slot />
            </div>
        </article>
    </body>
</head>
