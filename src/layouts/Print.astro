---
import QRCode from "../components/QRCode.astro";
const { title, labels, prose = "prose-lg" } = Astro.props.frontmatter;
---

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>{title}</title>
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
        rel="shortcut icon"
        type="image/x-icon"
        href="https://images.squarespace-cdn.com/content/v1/6226544384704c71b90fb38e/8fa3a4b8-5596-45a1-9e3a-ea10290604c7/favicon.ico?format=100w"
    />
    <link
        href="https://fonts.googleapis.com/css2?family=M+PLUS+1p:wght@800;900&display=swap"
        rel="stylesheet"
    />
    <style is:global>
        @tailwind base;
        @tailwind components;

        @tailwind utilities;

        @page {
            size: auto;
            margin: 0mm;
        }
        .jp-bold {
            font-family: "M PLUS 1p", sans-serif;
            font-weight: 800;
        }

        h4 {
            @apply font-bold;
        }

        h5 {
            @apply italic;
        }

        b {
            font-family: "M PLUS 1p", sans-serif;
            font-weight: 800;
        }

        @media print {
            a {
                text-decoration: none !important;
                color: black !important;
            }

            li {
                padding: 0;
            }
        }
        article img {
            @apply h-24 w-24;
        }
        article p:first {
            margin-top: 0 !important;
        }
    </style>
    <body class="p-4">
        <div class="prose prose-xl container mx-auto max-w-5xl">
            <h2 class="flex w-full items-baseline justify-between space-x-2">
                <span class="jp-bold">{title}</span>
                <a
                    class="flex flex-col items-center text-blue-500 no-underline print:visible print:h-8 print:w-8 print:text-black"
                    href="https://yasoprotivlenie.com"
                    target="_blank"
                >
                    <img
                        class="my-0 h-12 w-12"
                        src="data:image/png;base64,"
                        alt="homepage"
                    />
                    <span class="text-xs">HP</span>
                </a>
                <a
                    class="flex flex-col items-center text-blue-500 no-underline print:visible print:h-8 print:w-8 print:text-black"
                    href="https://twitter.com/yakinikuprotiv"
                    target="_blank"
                >
                    <img
                        class="my-0 h-12 w-12"
                        src="data:image/png;base64,"
                        alt="twitter"
                    />
                    <span class="text-xs">twitter</span>
                </a>
                <a
                    class="flex flex-col items-center text-blue-500 no-underline print:visible print:h-8 print:w-8 print:text-black"
                    href="https://www.instagram.com/ya.soprotivlenie/"
                    target="_blank"
                >
                    <img
                        class="my-0 h-12 w-12"
                        src="data:image/png;base64,"
                        alt="instagram"
                    />
                    <span class="text-xs">insta</span>
                </a>
                <a
                    class="flex flex-col items-center text-blue-500 no-underline print:visible print:h-8 print:w-8 print:text-black"
                    href="https://t.me/yasoprotivlenie"
                    target="_blank"
                >
                    <img
                        class="my-0 h-12 w-12"
                        src="data:image/png;base64,"
                        alt="telegram"
                    />
                    <span class="text-xs">tg</span>
                </a>
            </h2>
        </div>
        <article
            class:list={[prose, true]}
            class="prose container mx-auto mt-4 max-w-5xl"
            style="column-count: 2; column-gap: 20px;"
        >
            <slot />
            <QRCode
                title={labels[0]}
                href="https://rfu2022.org"
                src="data:image/png;base64,"
            />
            <QRCode
                title={labels[1]}
                href="https://secure.projecthope.org/site/Donation2;jsessionid=00000000.app30018b?8330.donation=form1&idb=1538239457&df_id=8330&mfc_pref=T&NONCE_TOKEN=A364845E87E0ACA49B8B3E58C97DC99F"
                hrefLabel="https://projecthope.org"
                src="data:image/png;base64,"
            />
            <QRCode
                title={labels[2]}
                href="https://www.crowdfunder.co.uk/p/tickets-for-ukrainian-refugees-to-leave-russia"
                src="data:image/png;base64,"
            />
        </article>
    </body>
</head>
